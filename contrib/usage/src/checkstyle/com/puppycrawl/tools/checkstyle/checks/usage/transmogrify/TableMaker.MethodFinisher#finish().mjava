    /**
    *
    * Completes a method by setting all references to return types,
    * signatures and exceptions.
    * @return <code>void</code>
    * @see #setReturnType()
    * @see #setSignature()
    * @see #setExceptionsThrown()
    */
    public void finish() {
      setReturnType();
      setSignature();
      setExceptionsThrown();
    }

