    /**
     * @see org.jaxen.Navigator#getParentNode(java.lang.Object)
     */
    public Object getParentNode(Object aObject)
    {
        if (aObject instanceof DetailAST) {
            return ((DetailAST) aObject).getParent();
        }
        else {
            return ((Attribute) aObject).getParent();
        }
    }

