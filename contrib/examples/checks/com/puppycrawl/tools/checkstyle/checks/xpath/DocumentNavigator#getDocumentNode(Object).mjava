    /* (non-Javadoc)
     * @see org.jaxen.Navigator#getDocumentNode(java.lang.Object)
     */
    public Object getDocumentNode(Object aObject)
    {
        if (isDocument(aObject)) {
            return aObject;
        }
        else {
            return getDocumentNode(getParentNode(aObject));
        }
    }

