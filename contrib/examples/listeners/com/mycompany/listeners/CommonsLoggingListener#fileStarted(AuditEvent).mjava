    /** @see com.puppycrawl.tools.checkstyle.api.AuditListener */
    public void fileStarted(AuditEvent aEvt)
    {
        if (mInitialized) {
            final Log log = mLogFactory.getInstance(Checker.class);
            log.info("File \"" + aEvt.getFileName() + "\" started.");
        }
    }

