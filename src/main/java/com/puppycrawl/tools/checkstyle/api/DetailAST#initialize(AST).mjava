    @Override
    public void initialize(AST ast)
    {
        final DetailAST da = (DetailAST) ast;
        setText(da.getText());
        setType(da.getType());
        lineNo = da.getLineNo();
        columnNo = da.getColumnNo();
        hiddenAfter = da.getHiddenAfter();
        hiddenBefore = da.getHiddenBefore();
    }

