    /** {@inheritDoc} */
    @Override
    public boolean equals(Object o) {
        if (this == o) {
            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        final LineColumn lineColumn = (LineColumn) o;
        return Objects.equals(line, lineColumn.line)
                && Objects.equals(col, lineColumn.col);
    }

