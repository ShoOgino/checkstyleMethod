    /** {@inheritDoc} */
    @Override
    public boolean equals(Object o)
    {
        if (this == o) {
            return true;
        }
        if (o == null || getClass() != o.getClass()) {
            return false;
        }
        final LineColumn that = (LineColumn) o;
        return Objects.equals(line, that.line)
                && Objects.equals(col, that.col);
    }

