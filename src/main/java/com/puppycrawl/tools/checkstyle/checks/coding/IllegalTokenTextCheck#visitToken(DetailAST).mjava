    @Override
    public void visitToken(DetailAST ast) {
        final String text = ast.getText();
        if (getRegexp().matcher(text).find()) {
            String message = getMessage();
            if (message.isEmpty()) {
                message = MSG_KEY;
            }
            log(
                ast.getLineNo(),
                ast.getColumnNo(),
                message,
                getFormat());
        }
    }

