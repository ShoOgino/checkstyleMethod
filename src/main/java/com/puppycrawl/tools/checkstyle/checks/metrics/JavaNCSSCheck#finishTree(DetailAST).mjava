    @Override
    public void finishTree(DetailAST aRootAST)
    {
        //pop counter from the stack
        final Counter counter = mCounters.pop();

        final int count = counter.getCount();
        if (count > mFileMax) {
            log(aRootAST.getLineNo(), aRootAST.getColumnNo(), "ncss.file",
                    count, mFileMax);
        }
    }

