    @Override
    public void visitToken(DetailAST aAST)
    {
        final ExpressionHandler handler = mHandlerFactory.getHandler(this, aAST,
            mHandlers.peek());
        mHandlers.push(handler);
        try {
            handler.checkIndentation();
        }
        catch (final NullPointerException npe) {
            npe.printStackTrace();
        }
    }

