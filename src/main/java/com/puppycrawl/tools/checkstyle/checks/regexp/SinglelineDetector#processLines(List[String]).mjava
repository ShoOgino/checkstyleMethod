    /**
     * Processes a set of lines looking for matches.
     * @param lines the lines to process.
     */
    public void processLines(List<String> lines) {
        resetState();
        int lineNo = 0;
        for (String line : lines) {
            lineNo++;
            checkLine(lineNo, line, options.getPattern().matcher(line), 0);
        }
        finish();
    }

