    @Override
    public void visitToken(DetailAST ast) {
        switch (ast.getType()) {
            case TokenTypes.EXPR:
                visitExpr(ast);
                break;
            case TokenTypes.SEMI:
                visitSemi(ast);
                break;
            case TokenTypes.FOR_INIT:
                inForHeader = true;
                break;
            default:
                break;
        }
    }

