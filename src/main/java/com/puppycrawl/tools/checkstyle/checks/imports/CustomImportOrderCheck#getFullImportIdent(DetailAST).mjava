    /**
     * Forms import full path.
     * @param aToken
     *        current token.
     * @return full path or null.
     */
    private static String getFullImportIdent(DetailAST aToken)
    {
        return aToken != null ? FullIdent.createFullIdent(aToken.
                findFirstToken(TokenTypes.DOT)).getText() : "";
    }

