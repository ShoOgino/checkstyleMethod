    @Override
    public String toString() {
        return "JavadocNodeImpl["
                + "index=" + index
                + ", type=" + JavadocUtils.getTokenName(type)
                + ", text='" + text + '\''
                + ", lineNumber=" + lineNumber
                + ", columnNumber=" + columnNumber
                + ", children=" + Objects.hashCode(children)
                + ", parent=" + parent + ']';
    }

