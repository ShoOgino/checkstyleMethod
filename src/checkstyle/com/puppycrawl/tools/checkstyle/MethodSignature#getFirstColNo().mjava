    /** @return the first column of the method signature **/
    int getFirstColNo()
    {
        if (mModSet.size() > 0) {
            return mModSet.getFirstColNo();
        }
        else if (mReturnType != null) {
            return mReturnType.getColumnNo();
        }
        else {
            return mName.getColumnNo();
        }
    }

