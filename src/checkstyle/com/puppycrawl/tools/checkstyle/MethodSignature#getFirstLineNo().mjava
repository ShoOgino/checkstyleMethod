    /** @return the first line of the method signature **/
    int getFirstLineNo()
    {
        if (mModSet.size() > 0) {
            return mModSet.getFirstLineNo();
        }
        else if (mReturnType != null) {
            return mReturnType.getLineNo();
        }
        else {
            return mName.getLineNo();
        }
    }

