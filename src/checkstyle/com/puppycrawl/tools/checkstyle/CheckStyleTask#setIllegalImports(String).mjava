    /** @param aPkgPrefixList comma separated list of package prefixes */
    public void setIllegalImports(final String aPkgPrefixList)
    {
        mOptionMemory.add(new Runnable()
            {
                public void run()
                {
                    mConfig.setIllegalImports(aPkgPrefixList);
                }
            });
    }

