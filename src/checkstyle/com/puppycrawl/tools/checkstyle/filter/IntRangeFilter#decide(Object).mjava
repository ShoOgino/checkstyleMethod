    /** @see com.puppycrawl.tools.checkstyle.api.Filter#decide */
    public int decide(Object aObject)
    {
        if ((mLowerBound.compareTo(aObject) <= 0)
            && (mUpperBound.compareTo(aObject) >= 0))
        {
            return Filter.ACCEPT;
        }
        else {
            return Filter.NEUTRAL;
        }
    }

