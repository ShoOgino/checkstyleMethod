    /** @see com.puppycrawl.tools.checkstyle.api.Check */
    public void leaveToken(DetailAST aAST)
    {
        switch (aAST.getType()) {
        case TokenTypes.LITERAL_IF:
            leaveLiteralIf(aAST);
            break;
        default:
            throw new IllegalStateException(aAST.toString());
        }
    }

