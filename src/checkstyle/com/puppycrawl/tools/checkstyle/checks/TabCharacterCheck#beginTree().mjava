    /** @see com.puppycrawl.tools.checkstyle.api.Check */
    public void beginTree()
    {
        final String[] lines = getLines();
        for (int i = 0; i < lines.length; i++) {
            final int tabPosition = lines[i].indexOf('\t');
            if (tabPosition != -1) {
                log(i + 1, tabPosition, "containsTab");
            }
        }
    }

