    private IClass resolveNew(
        SymTabAST newNode,
        Scope location,
        IClass context,
        boolean referencePhase) {

        IClass result;

        if (newIsConstructor(newNode)) {
            result =
                resolveConstructor(newNode, location, context, referencePhase);
        }
        else {
            result =
                resolveNewArray(newNode, location, context, referencePhase);
        }

        return result;
    }

