    /** @see com.puppycrawl.tools.checkstyle.api.Check */
    public void beginTree(DetailAST aRootAST)
    {
        final String[] lines = getLines();
        if (lines.length > mMaxFileLength) {
            log(1, "maxLen.file",
                    new Integer(lines.length),
                    new Integer(mMaxFileLength));
        }
    }

