    /**
     * {@inheritDoc}
     */
    public void leaveToken(DetailAST aAST)
    {
        if ((aAST.getType() == TokenTypes.VARIABLE_DEF)
            && ScopeUtils.isLocalVariableDef(aAST))
        {
            // we have handler only for members
            return;
        }
        mHandlers.pop();
    }

