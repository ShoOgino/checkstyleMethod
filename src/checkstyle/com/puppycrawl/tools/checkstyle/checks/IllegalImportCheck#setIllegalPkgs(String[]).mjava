    /**
     * Set the list of illegal packages.
     * @param aFrom array of illegal packages
     */
    public void setIllegalPkgs(String[] aFrom)
    {
        mIllegalPkgs.clear();
        for (int i = 0; i < aFrom.length; i++) {
            final String s = aFrom[i];
            mIllegalPkgs.add(s);
        }
    }

