    /** @see com.puppycrawl.tools.checkstyle.api.Check */
    public void visitToken(DetailAST aAST)
    {
        if (!inIgnoreList(aAST) && !isConstantDefinition(aAST)) {
            log(aAST.getLineNo(),
                aAST.getColumnNo(),
                "magic.number",
                aAST.getText());
        }
    }

