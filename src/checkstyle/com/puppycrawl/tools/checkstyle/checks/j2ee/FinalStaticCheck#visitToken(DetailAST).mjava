    /**
     * {@inheritDoc}
     */
    @Override
    public void visitToken(DetailAST aAST)
    {
        if (Utils.isInEJB(aAST)
            && Utils.isStatic(aAST)
            && !Utils.isFinal(aAST))
        {
            final DetailAST nameAST = aAST.findFirstToken(TokenTypes.IDENT);
            log(
                nameAST.getLineNo(),
                nameAST.getColumnNo(),
                "nonfinalstatic.bean", nameAST.getText());
        }
    }

