    /** @see java.lang.Comparable **/
    public int compareTo(Object aOther)
    {
        final LineText lt = (LineText) aOther;
        if (getLineNo() == lt.getLineNo()) {
            return 0;
        }
        else if (getLineNo() < lt.getLineNo()) {
            return -1;
        }
        else {
            return 1;
        }
    }

