    /**
     * {@inheritDoc}
     */
    @Override
    public int hashCode()
    {
        int result;
        result = mLineNo;
        result = HASH_MULT * result + mColNo;
        result = HASH_MULT * result + mKey.hashCode();
        for (Object element : mArgs) {
            result = HASH_MULT * result + element.hashCode();
        }
        return result;
    }

