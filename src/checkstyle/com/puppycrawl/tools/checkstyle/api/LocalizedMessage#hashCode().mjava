    /**
     * {@inheritDoc}
     */
    @Override
    public int hashCode()
    {
        int result;
        result = mLineNo;
        result = HASH_MULT * result + mColNo;
        result = HASH_MULT * result + mKey.hashCode();
        for (int i = 0; i < mArgs.length; i++) {
            result = HASH_MULT * result + mArgs[i].hashCode();
        }
        return result;
    }

