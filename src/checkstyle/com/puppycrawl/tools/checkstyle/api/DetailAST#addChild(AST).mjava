    /**
     * Adds new child to AST.
     * @param aAST the new child
     */
    @Override
    public void addChild(AST aAST)
    {
        super.addChild(aAST);
        if (aAST != null) {
            ((DetailAST) aAST).setParent(this);
            ((DetailAST) getFirstChild()).setParent(this);
        }
    }

