    /** @see java.lang.Object#hashCode() */
    public int hashCode()
    {
        int result;
        int textCode = (getText() == null) ? 0 : getText().hashCode();
        result = getType();
        result = HASH_MULT * result + textCode;
        result = HASH_MULT * result + getLineNo();
        result = HASH_MULT * result + getColumnNo();

        return result;
    }

