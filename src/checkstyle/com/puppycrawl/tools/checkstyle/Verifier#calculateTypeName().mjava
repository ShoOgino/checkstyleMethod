    /** Calcuates the current type name */
    private void calculateTypeName()
    {
        mCurrentTypeName = "";
        final Iterator it = mTypeNames.iterator();
        if (it.hasNext()) {
            mCurrentTypeName = (String) it.next();
        }

        while (it.hasNext()) {
            mCurrentTypeName += "." + (String) it.next();
        }
    }

