    @Test
    public void testVersionStringFormat() {
        final String[] text = {
            "/** ",
            " * @version 1.0",
            " */",
        };
        final List<TagInfo> tags = BlockTagUtils.extractBlockTags(text);
        assertEquals(1, tags.size());
        assertEquals("version", tags.get(0).getName());
        assertEquals("1.0", tags.get(0).getValue());
    }

