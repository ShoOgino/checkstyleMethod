    private static String getDefaultValueOfType(String propertyName,
                                                boolean isSpecialAllTokensType) {
        final String result;
        if (!isSpecialAllTokensType
                && (propertyName.endsWith("token") || propertyName.endsWith(
                "tokens"))) {
            result = " Default value is: ";
        }
        else {
            result = " Default value is ";
        }
        return result;
    }

