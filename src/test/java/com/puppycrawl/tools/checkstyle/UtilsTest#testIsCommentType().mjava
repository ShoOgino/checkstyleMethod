    @Test
    public void testIsCommentType() throws NoSuchMethodException {
        Assert.assertTrue(Utils.isCommentType(TokenTypes.SINGLE_LINE_COMMENT));
        Assert.assertTrue(Utils.isCommentType(TokenTypes.BLOCK_COMMENT_BEGIN));
