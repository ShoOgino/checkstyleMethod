    @Test
    public void testGetFirstNode2() {
        final DetailAST child = new DetailAST();
        child.setLineNo(6);
        child.setColumnNo(5);

        final DetailAST root = new DetailAST();
        root.setLineNo(5);
        root.setColumnNo(6);

        root.addChild(child);

        assertEquals("Unexpected node", root, CheckUtils.getFirstNode(root));
    }

