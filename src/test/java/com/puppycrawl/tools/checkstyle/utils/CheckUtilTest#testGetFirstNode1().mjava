    @Test
    public void testGetFirstNode1() {
        final DetailAST child = new DetailAST();
        child.setLineNo(5);
        child.setColumnNo(6);

        final DetailAST root = new DetailAST();
        root.setLineNo(5);
        root.setColumnNo(6);

        root.addChild(child);

        assertEquals("Unexpected node", root, CheckUtil.getFirstNode(root));
    }

