    private static boolean isAstEquals(DetailAST expected, DetailAST actual) {
        boolean result;
        if (expected == actual) {
            result = true;
        }
        else if (actual == null || expected == null) {
            result = false;
        }
        else {
            result = isAstEqualsSafe(expected, actual);
        }
        if (!result) {
            System.out.println("Expected: " + expected + " | Actual: " + actual);
        }
        return result;
    }

